{
  "password_policies": {
    "uppercase_letter_one": "[A-Z]",
    "lowercase_letter_one": "[a-z]",
    "numeric_character_one": "\\d",
    "special_character_one": "[!@#$%^&*(),.?\":{}|<>]",
    "minimum_length_four": "^.{4,}$",
    "minimum_length_eighth": "^.{8,}$"
  },
  "password_policy_groups": {
    "default": [
      "minimum_length_four"
    ]
  },
  "disallowed_domains": [
    "x.com"
  ]
}
